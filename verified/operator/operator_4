a, b, c = input("Введите длину коробки "), input("Введите ширину коробки "), input("Введите высоту коробки ")
x, y, z = input("Введите длину ящика "), input("Введите ширину ящика "), input("Введите высоту ящика ")
if ((a < x) and (b < y) and (c < z)) or ((a < z) and (b < y) and (c < x)) or ((a < x) and (b < z) and (c < y)):
    print("Коробка пройдет в ящик")
else:
    print("Коробка не пройдет в ящик")
