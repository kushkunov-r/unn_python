a = int(input("Введите число: "))
print("Делители (в диапазоне от 2 до 10) введенного:")
if (a % 2 == 0):
    print("2")
if (a % 3 == 0):
    print("3")
if (a % 4 == 0):
    print("4")
if (a % 5 == 0):
    print("5")
if (a % 6 == 0):
    print("6")
if (a % 7 == 0):
    print("7")
if (a % 8 == 0):
    print("8")
if (a % 9 == 0):
    print("9")
if (a % 10 == 0):
    print("10")
